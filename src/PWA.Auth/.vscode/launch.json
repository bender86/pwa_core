{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch PWA.Auth",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": ["run", "--project", "${workspaceFolder}/PWA.Auth.csproj"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}